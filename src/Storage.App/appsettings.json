{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "andyx-storage -> {Timestamp:HH:mm:ss} [{Level}] {Message}{NewLine}{Exception}"
        }
      }
    ]
  },

  "XRemote": [
    {
      "ServiceUrl": "",
      "Subscription": "",
      "JwtToken": ""
    }
  ],

  "DataStorage": {
    "Name": "default",
    "Status": "Active"
  },

  "AllowedHosts": "*"
}
