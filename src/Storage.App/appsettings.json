{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss}  andyx-storage  {Level,-12} {0,-3} | {0,-3} {Message}{NewLine}{Exception}"
        }
      }
    ]
  },

  "XNodes": [
    {
      "ServiceUrl": "localhost:9001",
      //"ServiceUrl": "dev-eu-andyx-buildersoft.azurewebsites.net",
      "Subscription": 1,
      "JwtToken": "na"
    }
  ],

  "DataStorage": {
    "Name": "default",
    "Status": "Active"
  },

  "Agent": {
    "MaxNumber": 4,
    "MinNumber": 1,
    "LoadBalanced": false
  },

  "Partition": {
    "Size": 3000
  },

  "Credentials": {
    "Username": "admin",
    "Password": "admin"
  },

  "AllowedHosts": "*"
}
