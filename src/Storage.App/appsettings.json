{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:HH:mm:ss}|[{Level}]|{Message}{NewLine}{Exception}"
        }
      }
    ]
  },

  "XNodes": [
    {
      "ServiceUrl": "locahost:9001",
      "Subscription": 1,
      "JwtToken": "na"
    }
  ],

  "DataStorage": {
    "Name": "default",
    "Status": "Active"
  },

  "Agent": {
    "MaxNumber": 4,
    "MinNumber": 1,
    "LoadBalanced": true
  },

  "Partition": {
    "Size": 4
  },

  "AllowedHosts": "*"
}
