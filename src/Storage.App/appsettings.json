{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss}  andyx-storage  {Level,-12} {0,-3} | {0,-3} {Message}{NewLine}{Exception}"
        }
      }
    ]
  },

  "XNodes": [
    {
      "ServiceUrl": "https://localhost:6541",
      "Subscription": 1,
      "JwtToken": "na",
      "Username": "admin",
      "Password": "admin"
      //"CertificateFile": "CERT_FILE",
      //"CertificatePassword": "CERT_PASSWORD"
    }
  ],

  "DataStorage": {
    "Name": "default",
    "Status": "Active"
  },

  "Agent": {
    "MaxNumber": 4,
    "MinNumber": 1,
    "LoadBalanced": false
  },

  "Partition": {
    "SizeInMemory": 3000,
    "FlushInterval": 5000,
    "PointerAcknowledgedMessageArchivationInterval": 3600000
    //"PointerAcknowledgedMessageArchivationInterval": 180000
  },

  "Credentials": {
    "Username": "admin",
    "Password": "admin"
  },

  "AllowedHosts": "*"
}
